{
    "author": "SG",
    "browser_action": {
       "default_icon": "icon.png",
       "default_popup": "main.html",
       "default_title": "Record Your Screen, Tab or Camera"
    },
    "content_security_policy": "script-src 'self' https://apis.google.com; object-src 'self'",
    "description": "Record screen activity, camera, tab, speakers in 4K HD video format",
    "icons": {
       "16": "icon.png",
       "22": "icon.png",
       "32": "icon.png",
       "48": "icon.png"
    },
    "manifest_version": 2,
    "minimum_chrome_version": "49",
    "name": "TestScreenViever",
    "permissions": [ "desktopCapture", "storage", "tabs", "tabCapture", "activeTab", "identity", "https://www.googleapis.com/*" ],
    "update_url": "https://clients2.google.com/service/update2/crx",
    "version": "1.0",
    "web_accessible_resources": [ "icon.png" ]
 }